<h1>Listing timelogs</h1>

<table>
  <thead>
    <tr>
      <th>Rate</th>
      <th>Date</th>
      <th>Nature work</th>
      <th>Hours worked</th>
      <th>Total</th>
      <th>Employee</th>
      <th>Work order</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timelogs.each do |timelog| %>
      <tr>
        <td><%= timelog.rate %></td>
        <td><%= timelog.date %></td>
        <td><%= timelog.nature_work %></td>
        <td><%= timelog.hours_worked %></td>
        <td><%= timelog.total %></td>
        <td><%= timelog.employee %></td>
        <td><%= timelog.work_order %></td>
        <td><%= timelog.user %></td>
        <td><%= link_to 'Show', timelog %></td>
        <td><%= link_to 'Edit', edit_timelog_path(timelog) %></td>
        <td><%= link_to 'Destroy', timelog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timelog', new_timelog_path %>

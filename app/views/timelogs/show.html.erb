<%- model_class = Timelog -%>
<div class="page-header">
  <h1><%=t '.title', :default => Timelog %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:rate) %></dt>
      <dd><pre class="prettyprint"><%= @timelog.rate %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:date) %></dt>
      <dd><pre class="prettyprint"><%= @timelog.date %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nature_work) %></dt>
      <dd><pre class="prettyprint"><%= @timelog.nature_work %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:hours_worked) %></dt>
      <dd><pre class="prettyprint"><%= @timelog.hours_worked %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:total) %></dt>
      <dd><pre class="prettyprint"><%= @timelog.total %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:employee) %></dt>
      <dd><pre class="prettyprint"><%= @timelog.employee %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              timelogs_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_timelog_path(@timelog), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              timelog_path(@timelog),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>


<h1>Listing mileagelogs</h1>

<table>
  <thead>
    <tr>
      <th>Starting mile</th>
      <th>Ending mile</th>
      <th>Rate</th>
      <th>Total miles</th>
      <th>Mile total cost</th>
      <th>Work order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mileagelogs.each do |mileagelog| %>
      <tr>
        <td><%= mileagelog.starting_mile %></td>
        <td><%= mileagelog.ending_mile %></td>
        <td><%= mileagelog.rate %></td>
        <td><%= mileagelog.total_miles %></td>
        <td><%= mileagelog.mile_total_cost %></td>
        <td><%= mileagelog.work_order %></td>
        <td><%= link_to 'Show', mileagelog %></td>
        <td><%= link_to 'Edit', edit_mileagelog_path(mileagelog) %></td>
        <td><%= link_to 'Destroy', mileagelog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mileagelog', new_mileagelog_path %>
